<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="id.android.kmabsensi.presentation.partner.evaluation.EvaluationCollaborationDetailActivity">

    <include layout="@layout/toolbar"/>

    <androidx.core.widget.NestedScrollView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@+id/toolbar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/_16dp">

            <TextView
                android:id="@+id/labelJudul"
                style="@style/tv14spf48070_avenir_heavy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="31dp"
                android:layout_marginStart="24dp"
                android:text="@string/judul"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <androidx.appcompat.widget.AppCompatEditText
                android:id="@+id/edtJudul"
                style="@style/tv14sp000000_avenir_medium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:hint="@string/judul"
                android:text="@string/evaluasi_berhenti_kolaborasi"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/labelJudul" />

            <TextView
                android:id="@+id/labelNama"
                style="@style/tv14spf48070_avenir_heavy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:layout_marginStart="24dp"
                android:text="@string/nama"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/edtJudul" />

            <androidx.appcompat.widget.AppCompatEditText
                android:id="@+id/edtNama"
                style="@style/tv14sp000000_avenir_medium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:hint="@string/nama"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/labelNama" />

            <TextView
                android:id="@+id/labelNoPartner"
                style="@style/tv14spf48070_avenir_heavy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:layout_marginStart="24dp"
                android:text="@string/nomor_partner"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/edtNama" />

            <androidx.appcompat.widget.AppCompatEditText
                android:id="@+id/edtNoPartner"
                style="@style/tv14sp000000_avenir_medium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:hint="@string/nomor_partner"
                android:inputType="number"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/labelNoPartner" />

            <TextView
                android:id="@+id/labelLeader"
                style="@style/tv14spf48070_avenir_heavy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:layout_marginStart="24dp"
                android:text="Leader"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/edtNoPartner" />

            <androidx.appcompat.widget.AppCompatEditText
                android:id="@+id/edtLeader"
                style="@style/tv14sp000000_avenir_medium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/labelLeader" />


            <TextView
                android:id="@+id/labelAlasan"
                style="@style/tv14spf48070_avenir_heavy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:layout_marginStart="24dp"
                android:text="@string/alasan_utama_berhenti_berkolaborasi"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/edtLeader" />

            <androidx.appcompat.widget.AppCompatEditText
                android:id="@+id/edtAlasanBerhentiKolaborasi"
                style="@style/tv14sp000000_avenir_medium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:hint="Masukkan Alasan"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:inputType="textMultiLine"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/labelAlasan" />

            <TextView
                android:id="@+id/labelSaran"
                style="@style/tv14spf48070_avenir_heavy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:layout_marginStart="24dp"
                android:text="@string/saran_perbaikan"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/edtAlasanBerhentiKolaborasi" />

            <androidx.appcompat.widget.AppCompatEditText
                android:id="@+id/edtSaranPerbaikan"
                style="@style/tv14sp000000_avenir_medium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:hint="Masukkan Saran"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:inputType="textMultiLine"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/labelSaran" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.core.widget.NestedScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>