<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="id.android.kmabsensi.presentation.admin.EvaluasiMenuFragment">

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/imageView22"
        android:layout_width="match_parent"
        android:layout_height="@dimen/toolbar_default"
        android:scaleType="fitXY"
        android:src="@drawable/bg_toolbar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/btnBack"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_8dp"
        android:padding="@dimen/_16dp"
        app:layout_constraintBottom_toBottomOf="@+id/labelPartner"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/labelPartner"
        android:background="?attr/selectableItemBackground"
        app:srcCompat="@drawable/ic_icon_back" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/labelPartner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Evaluasi"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/avenirltstd_heavy"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView22"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/imageView22"
        app:layout_constraintTop_toTopOf="@+id/imageView22" />

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/layoutMenu1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="16dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView22">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:visibility="visible">

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/btnEvaluasiLeader"
                android:layout_width="62dp"
                android:layout_height="62dp">

                <androidx.appcompat.widget.AppCompatImageView
                    android:layout_width="42dp"
                    android:layout_height="42dp"
                    android:layout_gravity="center"
                    app:srcCompat="@drawable/ic_evaluasi_leader" />

            </com.google.android.material.card.MaterialCardView>

            <TextView
                style="@style/tv13sp000000_avenir_heavy"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:gravity="center"
                android:text="Evaluasi Leader" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/btnEvaluasiKolaborasi"
                android:layout_width="62dp"
                android:layout_height="62dp"
                android:background="?attr/selectableItemBackground">

                <ImageView
                    android:layout_width="42dp"
                    android:layout_height="42dp"
                    android:layout_gravity="center"
                    android:src="@drawable/ic_evaluasi_kolaborasi" />

            </com.google.android.material.card.MaterialCardView>

            <TextView
                style="@style/tv13sp000000_avenir_heavy"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:gravity="center"
                android:text="Evaluasi Kolaborasi" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:visibility="invisible">

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/btnDataPartner"
                android:layout_width="62dp"
                android:layout_height="62dp">

                <androidx.appcompat.widget.AppCompatImageView
                    android:layout_width="42dp"
                    android:layout_height="42dp"
                    android:layout_gravity="center"
                    app:srcCompat="@drawable/ic_partner" />

            </com.google.android.material.card.MaterialCardView>

            <TextView
                style="@style/tv13sp000000_avenir_heavy"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:gravity="center"
                android:text="Data Partner" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:visibility="invisible">

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/btnPartnerCategory"
                android:layout_width="62dp"
                android:layout_height="62dp">

                <androidx.appcompat.widget.AppCompatImageView
                    android:layout_width="42dp"
                    android:layout_height="42dp"
                    android:layout_gravity="center"
                    app:srcCompat="@drawable/ic_kategori_partner" />

            </com.google.android.material.card.MaterialCardView>

            <TextView
                style="@style/tv13sp000000_avenir_heavy"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:gravity="center"
                android:text="Kategori Partner" />

        </LinearLayout>

    </androidx.appcompat.widget.LinearLayoutCompat>

</androidx.constraintlayout.widget.ConstraintLayout>