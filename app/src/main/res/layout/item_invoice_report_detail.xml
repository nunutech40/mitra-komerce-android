<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="?attr/selectableItemBackground"
    android:padding="@dimen/_12dp">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tvLeaderName"
        style="@style/tv16sp000000_avenir_heavy"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="@dimen/_12dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Leader Name" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tvReportValue"
        style="@style/tv14sp000000_avenir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="7 dari 20 Partner"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvLeaderName"
        tools:text="Namanya" />


</androidx.constraintlayout.widget.ConstraintLayout>
