<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="center">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txtNamaSdm"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        style="@style/tv10sp000000_avenir_medium"
        android:padding="8dp"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/guideline"
        />

    <View
        android:id="@+id/divider"
        android:layout_width="0.5dp"
        android:layout_height="0dp"
        android:background="@drawable/divider"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="@id/guideline"
        app:layout_constraintEnd_toEndOf="@id/guideline"
        app:layout_constraintBottom_toBottomOf="parent"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.1628" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txtLeads"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        style="@style/tv10sp000000_avenir_medium"
        android:padding="8dp"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/guideline"
        app:layout_constraintEnd_toStartOf="@id/guideline2"
        />

    <View
        android:id="@+id/divider2"
        android:layout_width="0.5dp"
        android:layout_height="0dp"
        android:background="@drawable/divider"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="@id/guideline2"
        app:layout_constraintEnd_toEndOf="@id/guideline2"
        app:layout_constraintBottom_toBottomOf="parent"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.2856" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txtTransaksi"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        style="@style/tv10sp000000_avenir_medium"
        android:padding="8dp"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/guideline2"
        app:layout_constraintEnd_toStartOf="@id/guideline3"
        />

    <View
        android:id="@+id/divider3"
        android:layout_width="0.5dp"
        android:layout_height="0dp"
        android:background="@drawable/divider"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="@id/guideline3"
        app:layout_constraintEnd_toEndOf="@id/guideline3"
        app:layout_constraintBottom_toBottomOf="parent"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.4284" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txtOrder"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        style="@style/tv10sp000000_avenir_medium"
        android:padding="8dp"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/guideline3"
        app:layout_constraintEnd_toStartOf="@id/guideline4"
        />

    <View
        android:id="@+id/divider4"
        android:layout_width="0.5dp"
        android:layout_height="0dp"
        android:background="@drawable/divider"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="@id/guideline4"
        app:layout_constraintEnd_toEndOf="@id/guideline4"
        app:layout_constraintBottom_toBottomOf="parent"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5712" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txtRatingKonversi"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        style="@style/tv10sp000000_avenir_medium"
        android:padding="8dp"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/guideline4"
        app:layout_constraintEnd_toStartOf="@id/guideline5"
        />

    <View
        android:id="@+id/divider5"
        android:layout_width="0.5dp"
        android:layout_height="0dp"
        android:background="@drawable/divider"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="@id/guideline5"
        app:layout_constraintEnd_toEndOf="@id/guideline5"
        app:layout_constraintBottom_toBottomOf="parent"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.7140" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txtRatingOrder"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        style="@style/tv10sp000000_avenir_medium"
        android:padding="8dp"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/guideline5"
        app:layout_constraintEnd_toStartOf="@id/guideline6"
        />

    <View
        android:id="@+id/divider6"
        android:layout_width="0.5dp"
        android:layout_height="0dp"
        android:background="@drawable/divider"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="@id/guideline6"
        app:layout_constraintEnd_toEndOf="@id/guideline6"
        app:layout_constraintBottom_toBottomOf="parent"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.8568" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txtNb"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        style="@style/tv10sp000000_avenir_medium"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/guideline6"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="Lihat"
        android:padding="8dp"
        android:gravity="center"
        android:textColor="#2196F3"
        />



</androidx.constraintlayout.widget.ConstraintLayout>