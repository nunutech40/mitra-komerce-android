<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/_16dp">


    <View
        android:id="@+id/imageView13"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/shape_circle_skeleton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/txtNamaSdm"
        android:layout_width="@dimen/_150sdp"
        android:layout_height="@dimen/_16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="14dp"
        android:background="@drawable/shape_skeleton"
        app:layout_constraintStart_toEndOf="@+id/imageView13"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/txtPekerjaan"
        android:layout_width="@dimen/_120sdp"
        android:layout_height="@dimen/_16dp"
        android:layout_marginTop="@dimen/_8dp"
        android:background="@drawable/shape_skeleton"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/txtNamaSdm"
        app:layout_constraintTop_toBottomOf="@+id/txtNamaSdm" />
</androidx.constraintlayout.widget.ConstraintLayout>