<!-- switch toggle-->
    <style name="scb_toggle" parent="Theme.MaterialComponents.Light">
        <!-- active thumb & track color (30% transparency) -->
        <item name="colorControlActivated">@color/cl_green_toggle</item>
        <item name="thumbTint">@color/white</item>

        <item name="trackColorInactive">@color/cl_orange</item>
        <!-- inactive thumb color -->
