<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:horizontalGap="0px"
    android:keyWidth="10%p"
    android:keyHeight="@dimen/key_height"
    android:verticalGap="0px">
    <Row>
        <Key
            android:keyWidth="25%p"
            android:keyEdgeFlags="left"
            android:keyLabel="1" />
        <Key
            android:keyWidth="25%p"
            android:keyLabel="2" />
        <Key
            android:keyWidth="25%p"
            android:keyLabel="3" />
        <Key
            android:codes="45"
            android:keyWidth="25%p"
            android:keyEdgeFlags="right"
            android:keyLabel="-" />
    </Row>

    <Row>
        <Key
            android:keyWidth="25%p"
            android:keyEdgeFlags="left"
            android:keyLabel="4" />
        <Key
            android:keyWidth="25%p"
            android:keyLabel="5" />
        <Key
            android:keyWidth="25%p"
            android:keyLabel="6" />

        <Key
            android:codes="32"
            android:keyWidth="25%p"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/sym_keyboard_space" />
    </Row>

    <Row>
        <Key
            android:keyWidth="25%p"
            android:keyEdgeFlags="left"
            android:keyLabel="7" />
        <Key
            android:keyWidth="25%p"
            android:keyLabel="8" />
        <Key
            android:keyWidth="25%p"
            android:keyLabel="9" />
        <Key
            android:codes="-5"
            android:isRepeatable="true"
            android:keyWidth="25%p"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/ic_delete_key" />
    </Row>

    <Row android:rowEdgeFlags="bottom">
        <Key
            android:codes="44"
            android:keyWidth="25%p"
            android:keyLabel="," />

        <Key
            android:keyWidth="25%p"
            android:keyLabel="0" />
        <!--
            android:codes: -101 is not a framework-defined key code but a key code that is
            privately defined in com.example.android.softkeyboard.LatinKeyboardView.
        -->
        <Key
            android:codes="46"
            android:keyWidth="25%p"
            android:keyLabel="." />
        <Key
            android:codes="-111"
            android:keyWidth="25%p"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/icon_done_orange" />
    </Row>
</Keyboard>